{
        "name": "httpie-client",
        "displayName": "HTTPie Client",
        "description": "HTTPie support for Visual Studio Code",
        "version": "0.0.4",
        "publisher": "anhkhoakz",
        "author": "anhkhoakz",
        "homepage": "https://github.com/anhkhoakz/vscode-httpie#readme",
        "repository": {
                "type": "git",
                "url": "https://github.com/anhkhoakz/vscode-httpie"
        },
        "bugs": {
                "url": "https://github.com/anhkhoakz/vscode-httpie/issues"
        },
        "keywords": [
                "httpie",
                "http",
                "rest",
                "client",
                "api",
                "vscode-extension",
                "code lens",
                "syntax highlighting"
        ],
        "icon": "assets/icon.png",
        "main": "./dist/extension.js",
        "license": "MIT",
        "engines": {
                "vscode": "^1.99.0",
                "node": "^22.0.0",
                "bun": "^1.0.0"
        },
        "categories": [
                "Other"
        ],
        "activationEvents": [
                "onCommand:httpie-client.request",
                "onLanguage:httpie"
        ],
        "contributes": {
                "languages": [
                        {
                                "id": "httpie",
                                "aliases": [
                                        "HTTPIE",
                                        "httpie",
                                        "Httpie"
                                ],
                                "extensions": [
                                        ".httpie"
                                ]
                        }
                ],
                "grammars": [
                        {
                                "language": "httpie",
                                "scopeName": "source.httpie",
                                "path": "./syntaxes/httpie.json"
                        }
                ],
                "snippets": [],
                "commands": [
                        {
                                "command": "httpie-client.request",
                                "title": "HTTPie: Send Request",
                                "category": "HTTPie"
                        }
                ]
        },
        "sideEffects": false,
        "scripts": {
                "vscode:prepublish": "bun run package",
                "compile": "webpack",
                "watch": "webpack --watch",
                "package": "webpack --mode production --devtool hidden-source-map",
                "compile-tests": "tsc -p . --outDir out",
                "watch-tests": "tsc -p . -w --outDir out",
                "pretest": "bun run compile-tests && bun run compile && bun run lint",
                "lint": "biome lint src/",
                "lint:fix": "biome lint src/ --write --unsafe",
                "format": "biome format src/",
                "format:fix": "biome format src/ --write",
                "test": "vscode-test",
                "prepare": "husky",
                "publish": "vsce package"
        },
        "devDependencies": {
                "@biomejs/biome": "^2.2.4",
                "@types/mocha": "^10.0.10",
                "@types/node": "^24.6.0",
                "@types/vscode": "^1.104.0",
                "@vscode/test-cli": "^0.0.11",
                "@vscode/test-electron": "^2.5.2",
                "husky": "^9.1.7",
                "ts-loader": "^9.5.4",
                "typescript": "^5.9.2",
                "webpack": "^5.102.0",
                "webpack-cli": "^6.0.1",
                "@types/bun": "latest"
        },
        "private": true
}
